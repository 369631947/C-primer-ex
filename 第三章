void main()
{
	string str;
	string largeStr;
	while (cin >> str)
	{
		if (largeStr.empty())
		{
			largeStr += str;
		}
		else
		{
			largeStr += " " + str;
		}
		if (str == "0")
		{
			break;
		}
	}

	cout << "The large string is " << largeStr << endl;
}
